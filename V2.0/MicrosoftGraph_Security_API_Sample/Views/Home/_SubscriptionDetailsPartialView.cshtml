@model MicrosoftGraph_Security_API_Sample.Models.SubscriptionResultModel

@if (String.IsNullOrWhiteSpace(Model.Error))
{
    <h2>Subscription details</h2>

    <div class="container-fluid" style="width: 100%;">

        <table class="table table-striped table-bordered table-hover">
            <thead class="thead-light">
                <tr>
                    <th>Property</th>
                    <th>Value</th>
                </tr>

            </thead>
            <tbody>
                <tr>
                    <td>ID</td>
                    <td>@Model.Id</td>
                </tr>
                <tr>
                    <td>Client State</td>
                    <td>@Model.ClientState</td>
                </tr>
                <tr>
                    <td>Change Type</td>
                    <td>@Model.ChangeType</td>
                </tr>
                <tr>
                    <td>Resource</td>
                    <td>@Model.Resource</td>
                </tr>
                <tr>
                    <td>Notification URL</td>
                    <td>@Model.NotificationUrl</td>
                </tr>
                <tr>
                    <td>Expiration DateTime</td>
                    <td>@Model.ExpirationDateTime</td>
                </tr>
            </tbody>
        </table>
    </div>
}
else
{
    <h2>Something went wrong!</h2>

    <div class="container-fluid" style="width: 100%;">

        <table class="table table-striped table-bordered">
            <thead class="thead-light">
                <tr>
                    <th>Property</th>
                    <th>Value</th>
                </tr>

            </thead>
            <tbody>
                <tr>
                    <td>Error</td>
                    <td>@Model.Error</td>
                </tr>
            </tbody>
        </table>
    </div>
}
